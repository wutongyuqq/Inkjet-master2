<?xml version="1.0" encoding="utf-8"?>
<me.samlss.ui.widget.QMUIWindowInsetLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/qmui_config_color_background"
    tools:context=".ui.MainActivity">

    <View
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"
        android:background="@color/app_header_color"
        android:fitsSystemWindows="true"></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="?actionBarSize"
        android:orientation="vertical">

        <include layout="@layout/layout_stick_header" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/layout_description"
                    android:background="?selectableItemBackground"
                    android:padding="10dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/tv_config_des"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/refer_print_device_settings"
                    android:textColor="@color/qmui_config_color_red"
                    android:textSize="16sp" />

                <ImageView
                    android:layout_marginLeft="5dp"
                    android:src="@mipmap/icon_qmark"
                    android:layout_width="20dp"
                    android:layout_height="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:padding="10dp">

                    <TextView
                        android:layout_width="120dp"
                        android:layout_height="wrap_content"
                        android:text="@string/send_msg_format"
                        android:textColor="@color/qmui_config_color_gray_2"
                        android:textSize="16sp" />

                    <FrameLayout
                        android:layout_marginLeft="10dp"
                        android:padding="5dp"
                        android:background="@drawable/sp_spinner"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                    <androidx.appcompat.widget.AppCompatSpinner
                        android:id="@+id/sp_send_msg_format"
                        android:overlapAnchor="false"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:entries="@array/sp_format">
                    </androidx.appcompat.widget.AppCompatSpinner>

                    </FrameLayout>
                </LinearLayout>

                <View
                    android:layout_alignParentBottom="true"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/qmui_config_color_gray_8" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:padding="10dp">

                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/end_character"
                        android:textColor="@color/qmui_config_color_gray_2"
                        android:textSize="16sp" />

                       <TextView
                           android:layout_marginLeft="5dp"
                           android:singleLine="true"
                           android:padding="5dp"
                           android:inputType="number"
                           android:imeOptions="actionDone"
                           android:id="@+id/et_end_character"
                           android:layout_width="100dp"
                           android:layout_height="wrap_content" />

                        <TextView
                            android:text="@string/ascii"
                            android:layout_marginLeft="10dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />

                </LinearLayout>

                <View
                    android:layout_alignParentBottom="true"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/qmui_config_color_gray_8" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:padding="10dp">

                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/print_start"
                        android:textColor="@color/qmui_config_color_gray_2"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_marginLeft="5dp"
                        android:singleLine="true"
                        android:padding="5dp"
                        android:imeOptions="actionDone"
                        android:id="@+id/et_print_start"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </LinearLayout>

                <View
                    android:layout_alignParentBottom="true"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/qmui_config_color_gray_8" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:padding="10dp">

                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/print_finished"
                        android:textColor="@color/qmui_config_color_gray_2"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_marginLeft="5dp"
                        android:singleLine="true"
                        android:padding="5dp"
                        android:imeOptions="actionDone"
                        android:id="@+id/et_print_finish"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </LinearLayout>

                <View
                    android:layout_alignParentBottom="true"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/qmui_config_color_gray_8" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:padding="10dp">

                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:text="@string/print_stop"
                        android:textColor="@color/qmui_config_color_gray_2"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_marginLeft="5dp"
                        android:singleLine="true"
                        android:padding="5dp"
                        android:imeOptions="actionDone"
                        android:id="@+id/et_print_stop"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </LinearLayout>

                <View
                    android:layout_alignParentBottom="true"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/qmui_config_color_gray_8" />

            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</me.samlss.ui.widget.QMUIWindowInsetLayout>